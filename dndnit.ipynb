    "import random\n",
    "\n",
    "stats = []\n",
    "\n",
    "for i in range(6):\n",
    "    rolls = []\n",
    "\n",
    "    for j in range(6):\n",
    "        roll = random.randint(1,6)\n",
    "        rolls.append(roll)\n",
    "\n",
    "stat = sum(rolls) - min(rolls)\n",
    "stats.append(stat)\n",
    "\n",
    "stats.remove(min(stats))\n",
    "\n",
    "\n",
    "stats = []\n",
    "for _ in range(6):\n",
    "    rolls = [random.randint(1, 6) for _ in range(4)]\n",
    "    stats.append(sum(rolls) - min(rolls))\n",
    "stats.remove(min(stats))\n",
    "print (stats)"
